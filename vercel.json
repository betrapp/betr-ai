{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/cron", "methods": ["POST"], "dest": "/api/index.ts" },
    { "src": "/(.*)", "dest": "/api/index.ts" }
  ],
  "functions": {
    "api/cron.ts": {
      "schedule": "0 0 * * *"
    }
  }
}
